{
  "routes": [
    {
      "route": "/static/*",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/*",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/static/*", "/images/*", "/*.{png,jpg,gif,ico,svg}"]
  }
} 