{
    "bindings": [
        {
            "authLevel": "anonymous",
            "type": "httpTrigger",
            "direction": "in",
            "name": "req",
            "methods": [
                "get",
                "post"
            ],
            "route": "{*route}"
        },
        {
            "type": "http",
            "direction": "out",
            "name": "$return"
        }
    ],
    "disabled": false,
    "scriptFile": "function_app.py",
    "entryPoint": "main",
    "authLevel": "function"
}